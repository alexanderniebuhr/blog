{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.options.json",
  "compilerOptions": {
    "outDir": "../../.moon/cache/types/packages/api",
    "allowImportingTsExtensions": true,
    "paths": {
      "@alexanderniebuhr/blog-db": [
        "../db/src/index.ts"
      ],
      "@alexanderniebuhr/blog-db/*": [
        "../db/src/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "*.ts"
  ],
  "references": [
    {
      "path": "../db"
    }
  ]
}
